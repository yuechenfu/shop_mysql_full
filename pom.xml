	<properties>
		<maven.compiler.source>1.7</maven.compiler.source>
		<maven.compiler.target>1.7</maven.compiler.target>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<spring.io.platform.version>Brussels-SR7</spring.io.platform.version>
	</properties>

	<repositories>
		<repository>
			<id>aliyun</id>
			<name>aliyun</name>
			<url>http://maven.aliyun.com/nexus/content/groups/public</url>
		</repository>
	</repositories>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-install-plugin</artifactId>
				<version>2.5.2</version>
				<executions>
					<execution>
						<id>install-alipay-sdk</id>
						<phase>clean</phase>
						<configuration>
							<file>${project.basedir}/libs/alipay-sdk-java20171026141113.jar</file>
							<groupId>com.alipay</groupId>
							<artifactId>alipay-sdk</artifactId>
							<version>20171026141113</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
						</configuration>
						<goals>
							<goal>install-file</goal>
						</goals>
					</execution>
					<execution>
						<id>install-kaptcha</id>
						<phase>clean</phase>
						<configuration>
							<file>${project.basedir}/libs/kaptcha-2.3.2.jar</file>
							<groupId>com.google.code.kaptcha</groupId>
							<artifactId>kaptcha</artifactId>
							<version>2.3.2</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
						</configuration>
						<goals>
							<goal>install-file</goal>
						</goals>
					</execution>
					<execution>
						<id>install-ojdbc</id>
						<phase>clean</phase>
						<configuration>
							<file>${project.basedir}/libs/ojdbc7.jar</file>
							<groupId>oracle</groupId>
							<artifactId>ojdbc</artifactId>
							<version>7</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
						</configuration>
						<goals>
							<goal>install-file</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

